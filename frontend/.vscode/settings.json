{
  "files.exclude": {
    "tests\\components\\node_modules": true,
    "*.npmrc": true,
    "*.npmrc.*": true,
    ".gitignore": true,
    ".npmrc": true,
    ".nuxt": true,
    ".output": true,
    ".vscode": true,
    "figma": true,
    "help": true,
    "node_modules": true,
    "public": true,
    "taailwind.config.ts": true,
    "yarn-error.log": true,
    "yarn.lock": true,
    "README.md": true
